https://leetcode.com/problems/maximum-nesting-depth-of-the-parentheses/description/


class Solution {
    public int maxDepth(String s) {
        int ans = 0;
        int cnt = 0;
        for(int i = 0; i < s.length(); i++)
        {
            if(s.charAt(i) == '(')
             ans = Math.max(ans, ++cnt); /// ++cnt hba noito uttar abe na

             if(s.charAt(i) == ')')
             cnt--;
        }
        return ans;
    }
}
